<query id="insertLog" action="insert">
    <tables>
        <table name="adminextend_admin_log" />
    </tables>
    <columns>
        <column name="module" var="module" />
        <column name="act" var="act" />
        <column name="request_vars" var="request_vars" />
        <column name="member_srl" var="member_srl" />
        <column name="ipaddress" var="ipaddress" default="ipaddress()" />
        <column name="regdate" var="regdate" default="curdate()" />
        <column name="is_authorized" var="is_authorized" />
    </columns>
</query>
