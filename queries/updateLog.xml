<query id="updateLog" action="update">
    <tables>
        <table name="adminextend_admin_log" />
    </tables>
    <columns>
            <column name="module" var="module" />
            <column name="act" var="act" />
            <column name="request_vars" var="request_vars" />
            <column name="member_srl" var="member_srl" />
            <column name="ipaddress" var="ipaddress" />
            <column name="regdate" var="regdate" />
            <column name="is_authorized" var="is_authorized" />
    </columns>
    <conditions>
            <condition operation="equal" column="log_idx" var="log_idx" filter="number" notnull="notnull" />
    </conditions>
</query>
